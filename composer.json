{
    "name": "schams-net/t3extensions-widget",
    "description": "Dashboard widget that displays the most recent TYPO3 extensions published/updated at the TER",
    "type": "typo3-cms-extension",
    "require": {
        "typo3/cms-core": "^12",
        "typo3/cms-dashboard": "^12",
        "php": "^8.1 || ^8.2 || ^8.3"
    },
    "autoload": {
        "psr-4": {
            "SchamsNet\\T3extensionsWidget\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "t3extensions_widget"
        }
    }
}
